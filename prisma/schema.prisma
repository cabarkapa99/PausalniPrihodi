generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Income {
  id      String   @id @default(uuid())
  userId  String
  amount  Float
  date    DateTime
  tag     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}